var inlineAdChoices=document.querySelector(".exp-ac--toggle-click-17-700 .ac--inline"),tooltip=document.querySelector(".ac__container"),semaphoreAdChoice=tooltip.querySelector(".ac__link.ac__link-semaphore");function getWindowWidth(){return window.innerWidth||document.documentElement.clientWidth||document.body.clientWidth}
function getPositionX(a){for(var b=0;a;)b=a===document.body?b+(a.offsetLeft-(a.scrollLeft||document.documentElement.scrollLeft)+a.clientLeft):b+(a.offsetLeft-a.scrollLeft+a.clientLeft),a=a.offsetParent;return b}
if(null!==inlineAdChoices&&null!==tooltip){var active=!1;inlineAdChoices.addEventListener("click",function(a){if(null!=semaphoreAdChoice&&"true"===semaphoreAdChoice.dataset.renderAdMenu)window.parent.postMessage(JSON.stringify({type:"AD_MENU",contentType:semaphoreAdChoice.dataset.contentType,creative:semaphoreAdChoice.dataset.creative,account:semaphoreAdChoice.dataset.account,matchedAt:semaphoreAdChoice.dataset.matchedAt}),window.location.origin),tooltip.style.display="none";else if(null!=semaphoreAdChoice&&
!0!==inlineAdChoices.semaphoreAttached&&"function"===typeof window.reportAd){inlineAdChoices.semaphoreAttached=!0;var b=tooltip.querySelector(".ac__link.ac__link-non-semaphore");tooltip.removeChild(b);semaphoreAdChoice.classList.toggle("ac__link-semaphore");var b=semaphoreAdChoice.dataset,c=b.account,d=b.creative,e=b.contentType;tooltip.addEventListener("click",function(a){a.preventDefault();window.reportAd(e,d,c)})}inlineAdChoices.classList.toggle("ac--inline--active");active?inlineAdChoices.classList.remove("ac--inline--open-left"):
(a.preventDefault(),tooltip.clientWidth>getWindowWidth()-getPositionX(inlineAdChoices)&&inlineAdChoices.classList.add("ac--inline--open-left"));active=!active})};